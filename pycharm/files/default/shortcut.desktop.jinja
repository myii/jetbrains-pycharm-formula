[Desktop Entry]
Encoding=UTF-8
Name={{ appname }}{{ '\ %sE'|format(edition) if edition is defined and edition else '' }}.app"
Comment={{ appname }}{{ '\ %sE'|format(edition) if edition is defined and edition else '' }}.app"
Icon={{ path }}/{{ command.split('.')[0] }}.png
Exec={{ path }}/{{ command }}
Terminal=false
Type=Application
